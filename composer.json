{
    "name": "hexlet/code",
    "description": "Вычислитель отличий",
    "type": "project",
    "bin": [
        "bin/gendiff"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src/"
        },
        "files": [
	    "src/Differ.php",
            "src/Parser.php",
            "src/Formatter.php",
            "src/Formatters/Json.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Plain.php",
            "tests/DiffTest.php"
        ]
    },
    "authors": [
        {
            "name": "Daria-Kop"
        }
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^5.4",
        "lstrojny/functional-php": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
